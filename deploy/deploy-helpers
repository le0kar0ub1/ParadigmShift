#
# Get Value of a stack output from key and stack name
# Parameters:
#   key to catch
#   stack name
#

function getValueFromKey() 
{
    keycatch=$1
    stackname=$2
    text=$(aws cloudformation describe-stacks --stack-name $stackname)
    line=$(echo "$text" | grep -in "\"OutputKey\": \"$keycatch\"," | cut -d : -f 1)
    let line=line+1
    value=$(echo "$text" | head -n $line | tail -1 | cut -d : -f 2- | xargs | cut -d , -f -1)
    echo $value
}
